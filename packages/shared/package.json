{
  "name": "@mc/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./constants/*": "./src/constants/*.ts",
    "./hooks/*": "./src/hooks/*.ts",
    "./types/*": "./src/types/*.ts",
    "./lib/*": "./src/lib/*.ts",
    "./utils": "./src/utils/index.ts",
    "./utils/*": "./src/utils/*.ts"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint ./src --max-warnings 0",
    "lint-staged": "lint-staged --concurrent false",
    "lint:fix": "eslint ./src --fix"
  },
  "devDependencies": {
    "@mc/config": "*",
    "@zenfs/core": "^2.0.0",
    "@zenfs/dom": "^1.1.6",
    "typescript": "^5.8.2"
  }
}
